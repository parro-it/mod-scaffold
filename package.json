{
  "name": "mod-scaffold",
  "version": "0.0.3",
  "description": "scaffold node packages",
  "repository": "parro-it/mod-scaffold",
  "author": "Andrea <andrea@parro.it>",
  "license": "MIT",
  "bin": {
    "create": "init.sh",
    "+": "init.sh"
  },
  "files": [
    "index.js",
    "init.sh",
    "example.js"
  ],
  "scripts": {
    "xo": "xo --ignore example.js --ignore index.js",
    "test": "npm run setup --silent && ava && xo --ignore example.js --ignore index.js",
    "setup": "rollup -f cjs main.js --output index.js",
    "cover-test": "npm run setup --silent && nyc ava && npm run xo --ignore example.js --ignore index.js",
    "cover-publish": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [],
  "tonicExampleFilename": "example.js",
  "jsnext:main": "main.js",
  "devDependencies": {
    "ava": "^0.16.0",
    "coveralls": "^2.11.14",
    "nyc": "^8.3.1",
    "rollup": "^0.36.3",
    "xo": "^0.17.0"
  }
}
